<UserControl x:Class="Anori.WPF.AttachedAncestorProperties.ManualUiTests.SingleSetter.Static.StaticEntryPointAndOneWayEndPointTextView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:singleSetter="clr-namespace:Anori.WPF.AttachedAncestorProperties.ManualUiTests.SingleSetter"
             Width="800"
             Height="450"
             mc:Ignorable="d">
    <StackPanel>
        <TextBlock>Entry Point</TextBlock>
        <StackPanel x:Name="StackPanel"
                    singleSetter:AttachedAncestorPropertyTextA.Setter="Attached Text">
            <TextBlock>End Point</TextBlock>
            <TextBox x:Name="TextBox1"
                     Text="{singleSetter:AttachedTextAGetterExtension}" />
        </StackPanel>
    </StackPanel>
</UserControl>