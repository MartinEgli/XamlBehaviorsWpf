<UserControl x:Class="Anori.WPF.AttachedAncestorProperties.ManualUiTests.SingleFatSetter.Controls.AttachedBindableUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:a="http://schemas.anori.com/xaml/attachedancestorproperties"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <Label IsEnabled="{a:AttachedBindingBooleanGetter}">Binding Boolean Test Text:</Label>
        <Label IsEnabled="{a:AttachedBindingBooleanGetter}">Boolean Test Text:</Label>
        <TextBox x:Name="EndPoint"
                 IsEnabled="{a:AttachedBindingBooleanGetter}"
                 Text="{a:AttachedBindingStringGetter}" />
    </StackPanel>
</UserControl>