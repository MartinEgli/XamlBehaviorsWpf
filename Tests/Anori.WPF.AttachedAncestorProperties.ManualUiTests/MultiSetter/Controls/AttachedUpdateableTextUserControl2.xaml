<UserControl x:Class="Anori.WPF.AttachedAncestorProperties.ManualUiTests.MultiSetter.AttachedUpdateableTextUserControl2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:attachedAncestorProperties="clr-namespace:Anori.WPF.AttachedAncestorProperties;assembly=Anori.WPF.AttachedAncestorProperties"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:guiTest="clr-namespace:Anori.WPF.AttachedAncestorProperties.ManualUiTests"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:multiSetter="clr-namespace:Anori.WPF.AttachedAncestorProperties.ManualUiTests.MultiSetter"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid x:Name="Grid">
        <StackPanel>
            <TextBox x:Name="EndPointA"
                     x:FieldModifier="public"
                     Text="{attachedAncestorProperties:Getter SetterProperty={x:Static multiSetter:AttachedAncestorProperties.SetterAProperty}}" />

            <TextBox x:Name="EndPointB"
                     x:FieldModifier="public"
                     Text="{attachedAncestorProperties:Getter SetterProperty={x:Static multiSetter:AttachedAncestorProperties.SetterBProperty}}" />
        </StackPanel>
    </Grid>
</UserControl>