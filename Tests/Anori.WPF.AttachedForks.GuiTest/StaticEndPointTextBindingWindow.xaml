<Window
    x:Class="Anori.WPF.AttachedAncestorProperties.GuiTest.StaticEndPointTextBindingWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:guiTest="clr-namespace:Anori.WPF.AttachedAncestorProperties.GuiTest"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="DynamicEndPointTextBindingWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock>Entry Point</TextBlock>
        <StackPanel x:Name="StackPanel" guiTest:AttachedAncestorPropertyTextA.Setter="Attached Text">
            <TextBlock>End Point</TextBlock>
            <TextBox
                x:Name="TextBox1"
                x:FieldModifier="public"
                Text="{guiTest:AttachedTextAGetterExtension}" />
        </StackPanel>
    </StackPanel>
</Window>